package-as: YOUR_ADDON
enable-nolib-creation: no

move-folders:
    YOUR_ADDON/YOUR_ADDON: YOUR_ADDON

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

license-output: LICENSE.txt

ignore:
    - CHANGELOG.md
    - CODE_OF_CONDUCT.md
    - LICENSE.md
    - README.md
    - .editorconfig
    - .github
